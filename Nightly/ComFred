<!DOCTYPE html>
<html>
<body>
  <!-- Generate a random percentage and outputline with weighted chances -->
  <script>
    const options = [
      { min: 91, max: 100, outputline: 'nightlyFredCheer' },
      { min: 81, max: 90, outputline: 'nightlyFredtheFashionable' },
      { min: 71, max: 80, outputline: 'nightlyForg' },
      { min: 61, max: 68, outputline: 'nightlyPat' },
      { min: 69, max: 69, outputline: 'nightlyKek' },
      { min: 70, max: 70, outputline: 'nightlyPat' },
      { min: 51, max: 60, outputline: 'nightlyFLOWER' },
      { min: 41, max: 50, outputline: 'nightlyPout' },
      { min: 31, max: 40, outputline: 'nightlyNotLikeThis' },
      { min: 21, max: 30, outputline: 'nightlyCry' },
      { min: 11, max: 20, outputline: 'nightlyAllGood' },
      { min: 1, max: 10, outputline: 'nightlyWHEEZE' }
    ];

    // Function to pick an option based on the random percentage
    function pickOptionByPercent(percent) {
      for (let i = 0; i < options.length; i++) {
        if (percent >= options[i].min && percent <= options[i].max) {
          return options[i];
        }
      }
    }

    // Generate a random percentage between 1 and 100
    const percent = Math.floor(Math.random() * 100) + 1;

    // Pick the corresponding outputline based on the percentage
    const pickedOption = pickOptionByPercent(percent);

    document.write(`${percent}% ${pickedOption.outputline}`);
  </script>
</body>
</html>
